{
  "downstream": {
    "url": "http://localhost:3001"
  },
  "activePreset": "default",
  "presets": [
    {
      "name": "default",
      "displayName": "Default Test User",
      "headers": {
        "X-User-Role": "user",
        "X-User-ID": "test-user-123"
      },
      "headerMappings": {},
      "oidcConfig": {
        "enabled": true,
        "subject": "test-user-123",
        "clientId": "test-client",
        "redirectUri": "http://localhost:8080/oauth2/callback",
        "scopes": ["openid", "profile", "email"],
        "tokenExpirationSeconds": 3600,
        "algorithm": "RS256",
        "keyId": "default",
        "email": "testuser@example.com",
        "name": "Test User",
        "preferredUsername": "testuser",
        "groups": ["users", "engineering"],
        "customClaims": {
          "role": "user",
          "department": "Engineering"
        }
      }
    },
    {
      "name": "admin",
      "displayName": "Test Admin User",
      "headers": {
        "X-User-Role": "admin",
        "X-User-ID": "admin-test-123"
      },
      "headerMappings": {},
      "oidcConfig": {
        "enabled": true,
        "subject": "admin-test-123",
        "clientId": "test-client",
        "redirectUri": "http://localhost:8080/oauth2/callback",
        "scopes": ["openid", "profile", "email", "admin"],
        "tokenExpirationSeconds": 7200,
        "algorithm": "RS256",
        "keyId": "default",
        "email": "testadmin@example.com",
        "name": "Test Admin User",
        "preferredUsername": "testadmin",
        "groups": ["users", "admins", "it"],
        "customClaims": {
          "role": "admin",
          "department": "IT"
        }
      }
    },
    {
      "name": "api_client",
      "displayName": "Test API Client",
      "headers": {
        "X-Client-Type": "api",
        "X-Client-ID": "api-client-test-123"
      },
      "headerMappings": {},
      "oidcConfig": {
        "enabled": true,
        "subject": "api-client-test-123",
        "clientId": "test-api-client",
        "redirectUri": "http://localhost:8080/oauth2/callback",
        "scopes": ["openid", "api"],
        "tokenExpirationSeconds": 1800,
        "algorithm": "RS256",
        "keyId": "default",
        "preferredUsername": "testapiclient",
        "groups": ["api-clients"],
        "customClaims": {
          "client_type": "api",
          "scope": "read write"
        }
      }
    }
  ],
  "routes": [
    {
      "pathPattern": "/api/**",
      "targetUrl": "http://localhost:3001",
      "description": "Test API routes",
      "enabled": true,
      "priority": 100
    }
  ],
  "reservedRoutes": [
    "/proxima/**",
    "/actuator/**",
    "/oauth2/**",
    "/.well-known/**"
  ]
}